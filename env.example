# .env.example

# Django settings
SECRET_KEY=your-production-secret-key-here
DEBUG=False

# Add your EC2 public IP and/or DNS name
ALLOWED_HOSTS=myprojectapp.it.com,www.myprojectapp.it.com
CSRF_TRUSTED_ORIGINS=https://myprojectapp.it.com,https://www.myprojectapp.it.com

# PostgreSQL settings
DATABASE_URL=postgres://postgres:yourpassword@db:5432/yourdbname

# Postgres container env vars (used in docker-compose.yml)
POSTGRES_DB=yourdbname
POSTGRES_USER=postgres
POSTGRES_PASSWORD=yourpassword

# Google OAuth (if using django-allauth with social login)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
