# Django Settings
SECRET_KEY='your-django-secret-key'
DEBUG=True

# Database Settings (used by django-environ DATABASE_URL)
# Format: postgres://USER:PASSWORD@HOST:PORT/DBNAME
DATABASE_URL='postgres://your_db_user:your_db_password@db:5432/your_db_name'

# Required by the Postgres container in Docker Compose
POSTGRES_DB=your_db_name
POSTGRES_USER=your_db_user
POSTGRES_PASSWORD=your_db_password

# Social Login via Google (for django-allauth)
GOOGLE_CLIENT_ID='your-google-oauth-client-id'
GOOGLE_CLIENT_SECRET='your-google-oauth-client-secret'
